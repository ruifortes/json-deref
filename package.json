{
  "name": "json-deref",
  "version": "1.0.11",
  "description": "Parses json-references",
  "main": "lib/jsonDeref_node.js",
  "files": [
    "lib/",
    "index.js"
  ],
  "scripts": {
    "build": "npm run build:node && npm run build:browser",
    "build:node": "cross-env BABEL_ENV=node babel index.js -o lib/jsonDeref_node.js --no-comments",
    "build:browser": "cross-env BABEL_ENV=browser babel index.js -o lib/jsonDeref_web.js --no-comments",
    "test": "tap test/*.js || true",
    "denode": "cross-env BABEL_ENV=node denode !denode.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruifortes/json-deref.git"
  },
  "keywords": [
    "json",
    "deref",
    "json-schema"
  ],
  "author": "ruisantosfortes@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ruifortes/json-deref/issues"
  },
  "homepage": "https://github.com/ruifortes/json-deref#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-conditional-compile": "0.0.5",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "cross-env": "^3.1.3",
    "tap": "^7.1.2"
  }
}
