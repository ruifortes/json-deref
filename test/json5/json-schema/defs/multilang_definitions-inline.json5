{
	$schema: 'http://json-schema.org/draft-04/schema#',
	description: 'Some common definitions',
	type: 'object',
	definitions: {
		id: {
			description: {
				en: 'Unique identifier.',
				'#ref': 'defs/translations#lang:id'
			},
			readOnly: true,
			format: 'uuid',
			example: '01234567-89ab-cdef-0123-456789abcdef',
			type: 'string',
			minLength: 1
		},
		email: {
			description: 'Email',
			format: 'email',
			readOnly: false,
			type: 'string',
			minLength: 1
		},
		password: {
			description: 'password',
			readOnly: false,
			type: 'string',
			minLength: 1
		}
	}
}